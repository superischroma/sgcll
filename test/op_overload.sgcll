blueprint point
{
    public x;
    public y;

    public constructor(x, y)
    {
        this.x = x;
        this.y = y;
    }
}

public operator(+=) point _(point p, i32 delta)
{
    p.x += delta;
    p.y += delta;
    return p;
}

i32 main()
{
    point p = point(5, 3);
    __builtin_i32_println(p.x);
    __builtin_i32_println(p.y);
    p += 5;
    __builtin_i32_println(p.x);
    __builtin_i32_println(p.y);
}