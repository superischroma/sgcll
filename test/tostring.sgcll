import "io";
import "string";

blueprint point
{
    public x;
    public y;

    public constructor(x, y)
    {
        this.x = x;
        this.y = y;
    }

    public string to_string()
    {
        return "point{x=" + (this.x -> i64) + ", y=" + (this.y -> i64) + "}";
    }
}

i32 main()
{
    point p = point(5, 3);
    io::println(p.to_string());
}