import "io";
import "string";

requirement stringable
{
    public string to_string();
}

blueprint point follows stringable
{
    public i32 x;
    public i32 y;

    public constructor(i32 x, i32 y)
    {
        this.x = x;
        this.y = y;
    }

    public string to_string()
    {
        return "point{x=" + (this.x -> i64) + ", y=" + (this.y -> i64) + "}";
    }
}

i32 main()
{
    point p = point(5, 3);
    print(p.to_string());
}